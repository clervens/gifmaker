<!doctype html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title><%= title %> | <%= short_description %></title>
	<meta name="keywords" content="generator, youtube, gif, webm, html5, nodejs, javascript, youtube gif maker,convert,youtube,gif,convert youtube to gif, yt2gif" />
	<meta name="description" content="<%= description %>" />
	<meta property="og:type" content="website" />
	<meta property="og:title" content="<%= title %> | free YouTube to GIF Generator" />
	<meta property="og:url" content="<%= req.protocol + '://' + req.get('host') + req.originalUrl %>" />
	<meta property="og:site_name" content="CVolcyGifMaker" />
	<meta property="og:description" content="<%= description %>" />
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css">
	<link rel="stylesheet" href="http://getbootstrap.com/examples/cover/cover.css">
	<link rel="stylesheet" href="/css/style.css">
</head>
<body>
	<div class="site-wrapper">

		<div class="site-wrapper-inner">

			<div class="cover-container">

				<div class="masthead clearfix">
					<div class="inner">
						<h3 class="masthead-brand"><%= title %></h3>
						<nav>
							<ul class="nav masthead-nav">
								<li class="active"><a href="/">Home</a></li>
								<!-- <li><a href="#">Features</a></li> -->
								<li><a href="/contact">Contact</a></Â li>
							</ul>
						</nav>
					</div>
				</div>

				<div class="inner cover">
					<h1 class="cover-heading"><%= title %></h1>
					<p class="lead"><%= description %></p>
					<div class="error-message" style="display:none"></div>
					<div class="video-url-sender">
						<form class="form-new-video-url">
							<div class="form-group">
								<div class="input-group">
									<input type="text" name="url" class="form-control form-text" placeholder="Youtube video URL" />
									<span class="input-group-addon form-submit">
										<a>Send</a>
									</span>
								</div>
								<small class="pull-left">ex. : https://www.youtube.com/watch?v=zKx2B8WCQuw</small>
								<div class="clearfix"></div>
								<div class="timerange-container">
									<label for="starttime" class="col-sm-4 col-md-2 control-label">Start Time</label>
									<div class="col-sm-8 col-md-4">
										<input type="text" name="starttime" placeholder="hh:mm:ss" class="form-control"></div>
										<label for="endtime" class="col-sm-4 col-md-2 control-label">End Time</label>
										<div class="col-sm-8 col-md-4">
											<input type="text" name="endtime" placeholder="hh:mm:ss" class="form-control">
										</div>
										<small class="pull-left">The duration can't exceed 30 seconds</small>
										<div class="clearfix"></div>
									</div>
								</div>
								<div class="formats-container">
									<div class="radio-inline">
										<label>
											<input type="radio" name="format" value="gif" checked>
											Gif
										</label>
									</div>
									<div class="radio-inline">
										<label>
											<input type="radio" name="format" value="webm">
											WebM
										</label>
									</div>
								</div>

							</form>
						</div>
						<section class="video-container">
							<div class="progress" style="display: none;">
								<div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">
									<span></span>
								</div>
							</div>
							<ul class="col-sm-4"></ul>
						</section>
					</div>

					<div class="mastfoot">
						<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
						<div class="leaderboard-abg visible-lg-block">
							<!-- Gif.cvolcy.com -->
							<ins class="adsbygoogle"
							style="display:inline-block;width:728px;height:90px"
							data-ad-client="ca-pub-5654969181576488"
							data-ad-slot="9700510422"></ins>
						</div>
						<script>
							(adsbygoogle = window.adsbygoogle || []).push({});
						</script>
						<div class="mobile-abg hidden-lg">
							<!-- Gif.cvolcy.com Mobile -->
							<ins class="adsbygoogle"
							style="display:inline-block;width:320px;height:100px"
							data-ad-client="ca-pub-5654969181576488"
							data-ad-slot="6328241622"></ins>
						</div>
						<script>
							(adsbygoogle = window.adsbygoogle || []).push({});
						</script>
						<div class="inner">
							<p>Gifmaker app on <a href="https://github.com/clervens/gifmaker" target="_blank">GitHub</a>, by <a href="https://github.com/clervens" target="_blank">Volcy, Clervens</a>.</p>
						</div>
					</div>

				</div>

			</div>

		</div>
		
		<script>
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
				(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
				m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			})(window,document,'script','//www.google-analytics.com/analytics.js','ga');
			ga('create', 'UA-52975090-1', 'auto');
			ga('send', 'pageview');
		</script>
		<script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
		<script>window.jQuery || document.write('<script src="/js/vendor/jquery-2.1.1.min.js"><\/script>')</script>
		<script src="/socket.io/socket.io.js"></script>
		<script src="/js/vendor/jquery-validation-1.13.1/jquery.validate.min.js"></script>
		<script src="/js/app.js"></script>
		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>
	</body>
	</html>
